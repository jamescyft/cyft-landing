# Example environment configuration for Cyft Landing Page
# Copy this file to .env and update with your values

# API Configuration (if you have a backend API)
VITE_API_URL=/api

# Google Sheets Integration
# Set to true to enable Google Sheets lead capture
VITE_GOOGLE_SHEETS_ENABLED=false
# Your Google Apps Script Web App URL (get this from Google Apps Script deployment)
VITE_GOOGLE_SHEETS_SCRIPT_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# Analytics (optional)
VITE_ENABLE_ANALYTICS=false
VITE_GTAG_ID=G-XXXXXXXXXX
VITE_SEGMENT_KEY=your_segment_write_key

# Error Tracking (optional)
VITE_ENABLE_ERROR_TRACKING=false
VITE_SENTRY_DSN=https://your_sentry_dsn@sentry.io/project_id
VITE_LOGROCKET_ID=your_logrocket_app_id

# Performance Monitoring (optional)
VITE_ENABLE_PERFORMANCE=false

# Debug Mode (shows additional logging)
VITE_DEBUG_MODE=false 